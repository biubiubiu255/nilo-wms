<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nilo.wms.dao.platform.DictionaryDao">

    <!-- 实体bean与数据字段对应关系 -->
    <resultMap id="dictionary" type="com.nilo.wms.dto.common.Dictionary">
        <result property="type" column="type" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
        <result property="code" column="code" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
        <result property="descC" column="desc_c" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
        <result property="descE" column="desc_e" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
    </resultMap>


    <select id="queryAll" resultMap="dictionary" statementType="PREPARED">
        SELECT *
        FROM t_sys_dictionary
        order by type ,show_order
    </select>

</mapper>